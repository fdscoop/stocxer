{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/static/index.html" },
    { "source": "/login", "destination": "/static/login.html" },
    { "source": "/login.html", "destination": "/static/login.html" },
    { "source": "/screener", "destination": "/static/screener.html" },
    { "source": "/screener.html", "destination": "/static/screener.html" },
    { "source": "/check_ml.html", "destination": "/static/check_ml.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "origin-when-cross-origin" }
      ]
    }
  ]
}
